FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type                    sets;
libs                    ("libsampling.so");
setFormat               raw;
interpolationScheme     cellPointFace;
fields
(
    U
);

sets
(
    line_x
    {
        type    lineCellFace;
        axis    x;
        start   (-1 0.5 0.5);
        end     (2 0.5 0.5);
    }

    line_y
    {
        type    lineCellFace;
        axis    y;
        start   (0.5 -1 0.5);
        end     (0.5 2 0.5);
    }
);

